(set-info :smt-lib-version 2.6)
(set-option :produce-assignments true)
(set-logic BPA)
(set-info :source | Kaya Guvendi and Ethan Bogle |)
(set-info :category "crafted")
(set-info :status sat)
(declare-const start Pointer)
(declare-const end Pointer)
(declare-const a Int)
(declare-const b Int)
(assert (= a (+ (Base (Block start)) (Offset start))))
(assert (not (= a 0)))
(assert (= b (+ (Base (Block end)) (Offset end))))
(assert (not (= b 0)))
(assert (= b (+ a (* 2 8))))
(assert (= (+p start 2) end))
(check-sat)
(exit)