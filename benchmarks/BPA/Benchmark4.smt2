(set-info :smt-lib-version 2.6)
(set-option :produce-assignments true)
(set-logic BPA)
(set-info :source | Kaya Guvendi and Ethan Bogle |)
(set-info :category "crafted")
(set-info :status unsat)
(declare-const a Int)
(declare-const b Int)
(declare-const p1 Pointer)
(declare-const p2 Pointer)
(declare-const temp Pointer)
(declare-fun Deref (Int Pointer) Int)
(assert (= (Deref 0 p1) a))
(assert (= (Deref 0 p2) b))
(assert (not (= a b)))
(assert (= (Deref 0 temp) (Deref 0 p1)))
(assert (= (Deref 1 p1) (Deref 0 p2)))
(assert (= (Deref 1 p2) (Deref 1 p2)))
(assert (= (Deref 1 temp) (Deref 0 temp)))
(assert (= (Deref 2 p2) (Deref 1 temp)))
(assert (= (Deref 2 p1) (Deref 1 p1)))
(assert (= (Deref 2 temp) (Deref 1 temp)))
(assert (not (and (= (Deref 2 p1) (Deref 0 p2)) (= (Deref 2 p2) (Deref 0 p1)))))
(check-sat)
(exit)