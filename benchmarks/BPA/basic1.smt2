(set-info :smt-lib-version 2.6)
(set-logic BPA)
(set-info :source | Kaya Guvendi and Ethan Bogle |)
(set-info :category "crafted")
(set-info :status sat)
(declare-fun f (Pointer) Pointer)
(declare-const x Pointer)
(declare-const y Pointer)
(assert (= (f x) y))
(assert (= (Offset x) (Offset y)))
(assert (not (= (+ (Base (Block x)) (Offset x)) (+ (Base (Block y)) (Offset y)))))
(check-sat)
(exit)
