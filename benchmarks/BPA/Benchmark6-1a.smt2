(set-info :smt-lib-version 2.6)
(set-option :produce-assignments true)
(set-logic BPA)
(set-info :source | Kaya Guvendi and Ethan Bogle |)
(set-info :category "crafted")
(set-info :status sat)
(declare-const arr Pointer)
(declare-const i0 Pointer)
(declare-const i1 Pointer)
(declare-const i2 Pointer)
(declare-const i3 Pointer)
(assert (=p i0 arr))
(assert (=p i1 (+p i0 1)))
(assert (=p i2 (+p i1 1)))
(assert (=p i3 (+p i2 1)))
(assert (not (and (<=p i0 (+p arr 2)) (<=p i1 (+p arr 2)) (<=p i2 (+p arr 2)))))
(check-sat)
(exit)